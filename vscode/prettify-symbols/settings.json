{
    "telemetry.enableTelemetry": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "calva.paredit.defaultKeyMap": "strict",
    "calva.prettyPrintingOptions": {
        "enabled": false,
        "width": 120,
        "maxLength": 50,
        "printEngine": "pprint"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "prettifySymbolsMode.substitutions": [
        {
            "language": "clojure",
            "prettifySymbolsMode.renderOn": "none",
            "prettifySymbolsMode.revealOn": "none",
            "prettifySymbolsMode.prettyCursor": "none",
             "prettifySymbolsMode.adjustCursorMovement": true,
            "substitutions": [
                {
                    "ugly": "rf/dispatch-sync",
                    "pre": "\\(",
                    "post": "\\s",
                    "pretty": "rf/~~>",
                    "style": {
                        "color": "#B877DBE6",
                    }
                },
                {
                    "ugly": "rf/subscribe",
                    "pre": "\\(",
                    "post": "\\s",
                    "pretty": "rf/<~~",
                    "style": {
                        "color": "#ffd633",
                    }
                },
                {
                    "ugly": "fn",
                    "pre": "\\(",
                    "post": "\\s",
                    "pretty": "λ",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "defn",
                    "pretty": "λ",
                     "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#ff3399"
                    }
                },
                {
                    "ugly": "defn-",
                    "pretty": "λ-",
                     "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#ff3399"
                    }
                },
                {
                    "ugly": "#{}",
                    "pretty": "ø",
                    "post": "",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "comp",
                    "pretty": "∘",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "partial",
                    "pretty": "Ƥ",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "def",
                    "pretty": "≡",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "set/union",
                    "pretty": "∪",
                    "pre": "",
                    "post": "",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "set/intersection",
                    "pretty": "∩",
                    "pre": "",
                    "post": "",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "set/subset?",
                       "pretty": "⊂",
                    "pre": "",
                    "post": "",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "set/difference",
                    "pretty": "\\-",
                    "pre": "",
                    "post": "",
                       "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "reduce",
                    "pretty": "∑",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "apply",
                    "pretty": "...",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "doseq",
                    "pretty": "∀",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "prop/for-all",
                    "pretty": "∀",
                    "pre": "\\(",
                    "post": "\\s",
                    "style": {
                        "color": "#B877DBE6"
                    }
                },
                {
                    "ugly": "symmetric-difference",
                    "pretty": "∆",
                    "pre": "",
                    "post": "",
                    "style": {
                        "color": "#B877DBE6"
                    }
                }
            ]
        }
    ],
    "editor.fontFamily": "'Jetbrains Mono','Fira Code','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.project.importHint": false,
    "redhat.telemetry.enabled": false,
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        },
        {
            "server": "localhost",
            "database": "",
            "authenticationType": "SqlLogin",
            "user": "sa",
            "password": "",
            "emptyPasswordInput": false,
            "savePassword": false
        }
    ],
    "calva.customREPLCommandSnippets": [
    
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "go.toolsManagement.autoUpdate": true,
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Cobalt2",
    "agdaMode.connection.agdaPath": "/usr/bin/agda",
    "github.gitAuthentication": false,
    "editor.semanticTokenColorCustomizations": null,
    "tabnine.experimentalAutoImports": true,
    "editor.tokenColorCustomizations": {
        "[Nord]": {
            "comments": "#93fccba1"
        }
    },        
    "workbench.preferredDarkColorTheme": "Nord",
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command",
        "push:command"
    ],
    "window.zoomLevel": -1,
    "editor.fontSize": 20,
    "workbench.startupEditor": "none",
    "[clojure]": {
        "editor.autoClosingBrackets": "always",
        "editor.autoClosingOvertype": "always",
        "editor.formatOnPaste": true
    },
    "peacock.affectPanelBorder": true,
    "githubPullRequests.createOnPublishBranch": "never",
    "sqltools.useNodeRuntime": true,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "d.dubCompiler": "dmd",
    "d.stdlibPath": "auto",
    "d.dubPath": "/usr/bin/dub",
    "explorer.confirmDelete": false,
    "d.servedPath": "/home/stuart/.local/share/code-d/bin/serve-d",
    "vsicons.dontShowNewVersionMessage": true,
    "FSharp.suggestGitignore": false,
    "editor.inlineSuggest.enabled": true,
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    "workbench.productIconTheme": "macos-modern",
    "github.copilot.advanced": {
    
    },
    "workbench.iconTheme": "vscode-icons",
    "files.saveConflictResolution": "overwriteFileOnDisk"
}
